{
  "name": "@salesforce/command",
  "version": "0.1.6",
  "description": "SFDX base command class",
  "main": "dist/exported",
  "license": "BSD-3-Clause",
  "scripts": {
    "typings": "./node_modules/.bin/tsc -p typings/tsconfig.json",
    "lint": "./node_modules/.bin/tslint -p .",
    "lint-report": "npm run lint -- -t checkstyle -o checkstyle.xml",
    "clean": "node scripts/clean.js",
    "compile": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "build": "npm run clean && npm run lint && npm run compile",
    "watch": "./node_modules/.bin/tsc -w -p tsconfig.json",
    "test": "./node_modules/.bin/mocha --require source-map-support/register --recursive dist/test/unit",
    "test-with-coverage": "npm run lint-report && npm run compile && scripts/unit-test-with-coverage.js",
    "prepare": "npm run build"
  },
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-command"
  ],
  "files": [
    "dist",
    "messages",
    "!dist/test/**/*",
    "dist/test/index.js",
    "!dist/**/*.map"
  ],
  "dependencies": {
    "@oclif/command": "1.4.2",
    "@oclif/errors": "^1.0.2",
    "@oclif/parser": "3.2.9",
    "@oclif/plugin-help": "^1.2.7",
    "@oclif/test": "^1.0.1",
    "@salesforce/core": "0.17.2",
    "lodash": "4.17.4"
  },
  "devDependencies": {
    "@oclif/config": "1.6.21",
    "@salesforce/dev-config": "1.0.4",
    "@types/chai": "4.0.4",
    "@types/lodash": "4.14.88",
    "@types/mocha": "2.2.42",
    "@types/node": "8.0.25",
    "chai": "4.1.1",
    "istanbul": "v1.1.0-alpha.1",
    "mocha": "5.0.1",
    "shelljs": "0.8.1",
    "sinon": "3.2.1",
    "source-map-support": "^0.5.3",
    "typescript": "2.9.2",
    "xunit-file": "1.0.0"
  },
  "types": "dist/exported.d.ts"
}
